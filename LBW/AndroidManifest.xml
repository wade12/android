<?xml version="1.0" encoding="utf-8"?>

<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.osgo"
    android:versionCode="1"
    android:versionName="1.0" >
    
    <uses-sdk
        android:minSdkVersion="8"
        android:maxSdkVersion="17" />
 
    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:allowBackup="true" >
        
        <activity android:name="com.osgo.MainActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity
            android:label="@string/app_name"
            android:name="com.osgo.DeliveryActivity" >
        </activity>
        
        <activity
            android:label="@string/app_name"
            android:name="com.osgo.BallPitchActivity" >
        </activity>
         
        <activity
            android:label="@string/app_name"
            android:name="com.osgo.OutsideOffActivity" >
        </activity>
        
        <activity
            android:label="@string/app_name"
            android:name="com.osgo.InLineActivity" >
        </activity>
        
        <activity
            android:label="@string/app_name"
            android:name="com.osgo.OutsideLegActivity" >
        </activity>
        
    </application>
    
</manifest>
